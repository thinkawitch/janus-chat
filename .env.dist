JANUS_ADMIN_SECRET=adminsecret
JANUS_TEXTROOM_ADMIN_KEY=textroomkey
JANUS_VIDEOROOM_ADMIN_KEY=videoroomkey

# coturn
COTURN_DOMAIN=coturn.tld
